{
	"env": {
		"browser": true,
		"commonjs": true,
		"es2020": true
	},
	"extends": [
		"airbnb-base",
		"prettier"
	],
	"plugins": [
		"prettier"
	],
	"parserOptions": {
		"ecmaVersion": 11
	},
	"rules": {
		"no-console": "off",
		"radix": "off",
		"no-restricted-syntax": [
			"error",
			"ForInStatement",
			"LabeledStatement",
			"WithStatement"
		],
		"class-methods-use-this": 0,
		"no-param-reassign":"off"
	}
}